<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
    <class name="bean.SolutionBean" table="Solution">
        <id name="solutionid" column="solutionid" type="string">
            <generator class="assigned"/>
        </id>
        <property name="solutionname" column="solutionname" type = "string" />
        <property name="solutionset" column="solutionset" type ="blob"/>
        <property name="datesubmited" column="datesubmited" type = "date"/>
    </class>
</hibernate-mapping>